{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "inlineSourceMap": true,
    "moduleResolution": "node",
    "pretty": true,
    "stripInternal": true,
    "noEmitHelpers": true,
    "noEmitOnError": false,
    "outDir": "ts-out",
    "typeRoots": [
        "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "www",
    "bower_components",
    "coverage"
  ],
  "compileOnSave": false
}